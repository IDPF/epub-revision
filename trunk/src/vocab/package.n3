@base  <http://www.idpf.org/epub/30/vocab/package/> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix zt:  <http://www.daisy.org/ns/rdf/property#> .
@prefix htu: <http://www.daisy.org/ns/rdf/usage/html/#> .                
@prefix dcterms:  <http://purl.org/dc/terms/> .
@prefix jena: <http://www.daisy.org/ns/rdf/jena#> .
@prefix foaf: <http://xmlns.com/foaf/spec/#term_> .
@prefix aria: <http://www.w3.org/WAI/ARIA/schemata/aria-1.rdf#> .
@prefix xhv: <http://www.w3.org/1999/xhtml/vocab/#> .
@prefix role: <http://www.w3.org/1999/xhtml/vocab#> .
@prefix : <#> .

<>
  dcterms:title "EPUB 3 Package Metadata Vocabulary" ;
  dcterms:description "This vocabulary, a part of EPUBÂ® 3.0, defines a set of metadata properties for use in [../../spec/epub30-publications.html#sec-package-documents Package Documents]." ;
  dcterms:publisher "International Digital Publishing Forum (IDPF)" ;
  dcterms:issued "2011-01-03" ;  
  dcterms:creator "mgylling" . 
    

########## package metadata #########

:package-props			a rdf:Bag ;
						jena:bag-sort "10" ;
						dcterms:title "Package-level Metadata" .


:content-version			a rdf:Property ;
						rdfs:label "content-version";
						rdf:member :package-props ;
						jena:prop-sort "10";
						zt:value "A string of the form #.#.#" ;
						rdfs:comment "The {content-version} property provides the version number of the publication. The semantics of the three digit sequences and how they are incremented for any given publication is not defined." .

:publication-type			a rdf:Property ;
						rdfs:label "publication-type";
						rdf:member :package-props ;
						jena:prop-sort "20";
						zt:value "dictionary | annotation" ;
						rdfs:comment "The {publication-type} property describes the nature of a specialized type of EPUB Package. The value {dictionary} indicates that the package contains a dictionary as defined in TODO and the value {annotation} that the package contains an annotation as defined in TODO." .

:source-identifier			a rdf:Property ;
						rdfs:label "source-identifier";
						rdf:member :package-props ;
						jena:prop-sort "30";
						zt:value "xsd:string" ;
						rdfs:comment "The {source-identifier} property references the {identifier} (element or property) that is the source of the NCX pageList (i.e., the publication from which the pagination was derived)." .

:page-progression-direction             a rdf:Property ;
						rdfs:label "page-progression-direction";
						rdf:member :package-props ;
						jena:prop-sort "40";
						zt:value "rtl | ltr" ;
						rdfs:comment "The {page-progression-direction} property indicates the global direction in which the publication content flows. This property can be overriden by individual manifest {items}s." .



########## ancillary metadata properties #########

:ancillary-props		          a rdf:Bag ;
						jena:bag-sort "20" ;
						dcterms:title "Ancillary metadata properties" .

:alternate-script			a rdf:Property ;
						rdfs:label "alternate-script";
						rdf:member :ancillary-props ;
						jena:prop-sort "10";
						zt:extends "all" ;
						zt:ex "creator" ;
						rdfs:comment "The {alternate-script} property provides an alternate expression of the associated property value in a language and script identified by the {xml:lang} attribute. The property is typically attached to {creator} and {title} properties for internationalization purposes." .

:date-ref				a rdf:Property ;
						rdfs:label "date-ref";
						rdf:member :ancillary-props ;
						jena:prop-sort "20";
						zt:extends "all" ;
						zt:value "xsd:IDREF" ;
						zt:ex "title" ;
						rdfs:comment "The {date-ref} property relates an {[[#rdfa-ancillary-property-event event]]} to the {date} on which it occurred. This property is typically used to tie changes to metadata to the corresponding revision date for the publication. !Why was this under metadata authority props? Is it global?^" .

:display-seq				a rdf:Property ;
						rdfs:label "display-seq";
						rdf:member :ancillary-props ;
						jena:prop-sort "30";
						zt:extends "all" ;
						zt:value "xsd:unsignedInt" ;
						zt:ex "title" ;
						rdfs:comment "The {display-seq} property indicates the numeric position in which to display the current property relative to identical metadata properties. For example, to indicate the order in which to render multiple {title} elements or properties." .

:event					a rdf:Property ;
						rdfs:label "event";
						rdf:member :ancillary-props ;
						jena:prop-sort "40";
						zt:extends "dcterms:date" ;
						zt:value "creation | meta-update | modification | publication | {xsd:string}" ;
						zt:ex "identifier" ;
						rdfs:comment "The {event} property indicates a significant event in the lifecycle of a publication. Events are typically modify {date} elements or properties to provide the reason for a change to the publication. A set of standard values is defined for this property, but no restrictions are imposed on the extension of this list." .

:file-as					a rdf:Property ;
						rdfs:label "file-as";
						rdf:member :ancillary-props ;
						jena:prop-sort "50";
						zt:extends "all" ;
						zt:ex "title" ;
						rdfs:comment "The {file-as} property provides the normalized form of the associated property for sorting." .

:role					a rdf:Property ;
						rdfs:label "role";
						rdf:member :ancillary-props ;
						jena:prop-sort "60";
						zt:extends "dcterms:creator" ;
						zt:extends "dcterms:contributor" ;
						zt:value "It is recommended that values be drawn from [[http://www.loc.gov/marc/relators/relacode.html MARC Code List for Relators]] and [[http://www.editeur.org/files/ONIX%203/ONIX_Code_Lists_Issue_12_Changes.pdf ONIX 3.0 Code List 17]]" ;
						zt:ex "creator" ;
						rdfs:comment "The {role} property describes the nature of work performed by a {creator} or {contributor}, for example that they represent the author or editor of a publication." .

:scheme				a rdf:Property ;
						rdfs:label "scheme";
						rdf:member :ancillary-props ;
						jena:prop-sort "70";
						zt:extends "dcterms:identifier" ;
						zt:extends "dcterms:subject" ;
						zt:value "Values for dcterms:subject should be drawn from [[http://www.editeur.org/files/ONIX%203/ONIX_Code_Lists_Issue_12_Changes.pdf ONIX 3.0 Code List 27]].!TODO: note different schemes for different types or leave unspecified?^" ;
						zt:ex "identifier" ;
						rdfs:comment "The {scheme} property identifies the formal system used to express the value of an {identifier} or {subject} element or property." .

:title-order				a rdf:Property ;
						rdfs:label "title-order";
						rdf:member :ancillary-props ;
						jena:prop-sort "80";
						zt:extends "dcterms:title" ;
						zt:value "xsd:unsignedInt" ;
						zt:ex "title" ;
						rdfs:comment "The {title-order} property indicates the numeric position in which to sort the associated {title} relative to other titles identified with the same {[[#rdfa-ancillary-property-title-type title-type]]} value. !was sort-order, but unclear if any other use for the property with display-seq, and whether this is already covered by file-as.^" .

:title-type				a rdf:Property ;
						rdfs:label "title-type";
						rdf:member :ancillary-props ;
						jena:prop-sort "90";
						zt:extends "dcterms:title" ;
						zt:value "{primary} | {short} | {long} | {sub} | {series} | {collection} | {edition} | {slang}" ;
						zt:ex "title" ;
						rdfs:comment "The {title-type} property indicates the form or nature of a {title}.  A set of standard values are defined for this property but no restrictions are imposed on the extension of this list. !isn't this a role?^" .



########## metadata authority properties #########

:meta-auth-props			a rdf:Bag ;
						jena:bag-sort "30" ;
						dcterms:title "Metadata Authority properties" .

:authority				a rdf:Property ;
						rdfs:label "authority";
						rdf:member :meta-auth-props ;
						jena:prop-sort "10";
						zt:value "xsd:IDREF" ;
						zt:ex "meta-auth" ;
						rdfs:comment "The {authority} property references the metadata authority ({[[#rdfa-meta-auth-property-meta-auth meta-auth]]}) responsible for an instance of package metadata." .

:meta-auth				a rdf:Property ;
						rdfs:label "meta-auth";
						rdf:member :meta-auth-props ;
						jena:prop-sort "20";
						zt:ex "meta-auth" ;
						rdfs:comment "The {meta-auth} property provides the name of a party or authority responsible for the package metadata." .

:signature				a rdf:Property ;
						rdfs:label "signature";
						rdf:member :meta-auth-props ;
						jena:prop-sort "30";
						zt:extends "meta-auth" ;
						zt:value "xsd:IDREF" ;
						zt:ex "meta-auth" ;
						rdfs:comment "The {signature} property relates a metadata authority ({[[#rdfa-meta-auth-property-meta-auth meta-auth]]}) responsible for creating or updating package metadata to an XML Signature contained in the Signatures.xml file." .


########## manifest properties #########

:manifest-props			a rdf:Bag ;
						jena:bag-sort "40" ;
						dcterms:title "Manifest properties" .

:cover-image				a rdf:Property ;
						rdfs:label "cover-image";
						rdf:member :manifest-props ;
						jena:prop-sort "10";
						zt:value "N/A" ;
						rdfs:comment "The {cover-image} property identifies a resource as the cover image for the publication." .

:mathml				a rdf:Property ;
						rdfs:label "mathml";
						rdf:member :manifest-props ;
						jena:prop-sort "20";
						zt:value "N/A" ;
						rdfs:comment "The {mathml} property indicates that the associated resource contains one or more instances of MathML markup." .

:online-resources			a rdf:Property ;
						rdfs:label "online-resources";
						rdf:member :manifest-props ;
						jena:prop-sort "30";
						zt:value "N/A" ;
						rdfs:comment "The {online-resources} property indicates that the associated resource references publication resources that are located online." .

:page-spread-left			a rdf:Property ;
						rdfs:label "page-spread-left";
						rdf:member :manifest-props ;
						jena:prop-sort "40";
						zt:value "N/A" ;
						rdfs:comment "The {page-spread-left} property indicates left-to-right directionality of a top-level SVG document. This property must always be used in conjunction with the {[[#rdfa-manifest-svg svg]]} property when describing a manifest {item}. It must not be used simultaneously with the {[[#rdfa-manifest-page-spread-right page-spread-right]]} property." .

:page-spread-right			a rdf:Property ;
						rdfs:label "page-spread-right";
						rdf:member :manifest-props ;
						jena:prop-sort "50";
						zt:value "N/A" ;
						rdfs:comment "The {page-spread-right} property indicates right-to-left directionality of a top-level SVG document. This property must always be used in conjunction with the {[[#rdfa-manifest-svg svg]]} property when describing a manifest {item}. It must not be used simultaneously with the {[[#rdfa-manifest-page-spread-left page-spread-left]]} property." .

:scripted				a rdf:Property ;
						rdfs:label "scripted";
						rdf:member :manifest-props ;
						jena:prop-sort "60";
						zt:value "N/A" ;
						rdfs:comment "The {scripted} property indicates that the associated resource contains scripted content." .

:svg					a rdf:Property ;
						rdfs:label "svg";
						rdf:member :manifest-props ;
						jena:prop-sort "70";
						zt:value "N/A" ;
						rdfs:comment "The {svg} property indicates that the associated resource contains one or more instances of SVG markup." .


########## metadata record properties #########

:metarec-props			a rdf:Bag ;
						jena:bag-sort "50" ;
						dcterms:title "Metadata record types" .

:onix-record				a rdf:Property ;
						rdfs:label "onix-record";
						rdf:member :metarec-props ;
						jena:prop-sort "10";
						zt:value "N/A" ;
						rdfs:comment "The {onix-record} property indicates the referenced resource is an ONIX record." .

:mods-record				a rdf:Property ;
						rdfs:label "mods-record";
						rdf:member :metarec-props ;
						jena:prop-sort "20";
						zt:value "N/A" ;
						rdfs:comment "The {mods-record} property indicates the referenced resource is a MODS record." .

:xmp-record				a rdf:Property ;
						rdfs:label "xmp-record";
						rdf:member :metarec-props ;
						jena:prop-sort "30";
						zt:value "N/A" ;
						rdfs:comment "The {xmp-record} property indicates the referenced resource is an XMP record." .

:marc21xml-record			a rdf:Property ;
						rdfs:label "marc21xml-record";
						rdf:member :metarec-props ;
						jena:prop-sort "40";
						zt:value "N/A" ;
						rdfs:comment "The {marc21xml-record} property indicates the referenced resource is a MARC21 record." .

