@base  <http://www.idpf.org/epub/30/vocab/package/> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix zt:  <http://www.daisy.org/ns/rdf/property#> .
@prefix htu: <http://www.daisy.org/ns/rdf/usage/html/#> .                
@prefix dcterms:  <http://purl.org/dc/terms/> .
@prefix jena: <http://www.daisy.org/ns/rdf/jena#> .
@prefix foaf: <http://xmlns.com/foaf/spec/#term_> .
@prefix aria: <http://www.w3.org/WAI/ARIA/schemata/aria-1.rdf#> .
@prefix xhv: <http://www.w3.org/1999/xhtml/vocab/#> .
@prefix role: <http://www.w3.org/1999/xhtml/vocab#> .
@prefix : <#> .

<>
  dcterms:title "EPUB3 Package Metadata Vocabulary" ;
  dcterms:description "This vocabulary, a part of EPUB 3.0, defines a set of metadata properties for use in [../../spec/epub30-publications.html#sec-package-documents Package Documents]." ;
  dcterms:publisher "International Digital Publishing Forum (IDPF)" ;
  dcterms:issued "2011-01-03" ;  
  dcterms:creator "mgylling" . 
    

########## package metadata #########

:package-props			a rdf:Bag ;
						jena:bag-sort "10" ;
						dcterms:title "Package-level Metadata" .


########## publication metadata #########

:publication-props			a rdf:Bag ;
						jena:bag-sort "20" ;
						rdf:member :package-props ;
						dcterms:title "Publication Metadata" .

:content-version			a rdf:Property ;
						rdfs:label "content-version";
						rdf:member :publication-props ;
						jena:prop-sort "10";
						rdfs:comment "The version number of the publication." .

:publication-type			a rdf:Property ;
						rdfs:label "publication-type";
						rdf:member : ;
						jena:prop-sort "20";
						rdfs:comment "The type of content the publication contains." .



########## extended metadata properties #########

:extended-props			a rdf:Bag ;
						jena:bag-sort "30" ;
						rdf:member :package-props ;
						dcterms:title "Extended metadata properties" .

:alternate-script			a rdf:Property ;
						rdfs:label "alternate-script";
						rdf:member :extended-props ;
						jena:prop-sort "10";
						rdfs:comment "Indicates an alternate representation of the associated property in a language and/or script identified by the xml:lang attribute." .

:date-ref				a rdf:Property ;
						rdfs:label "date-ref";
						rdf:member :meta-auth-props ;
						jena:prop-sort "20";
						rdfs:comment "Indicates a reference to a dcterms:date for the associated property." .

:display-seq				a rdf:Property ;
						rdfs:label "display-seq";
						rdf:member :extended-props ;
						jena:prop-sort "30";
						rdfs:comment "Indicates the sequential position in which to display the current property relative to similarly-identified properties for the publication." .

:event					a rdf:Property ;
						rdfs:label "event";
						rdf:member :extended-props ;
						jena:prop-sort "40";
						rdfs:comment "Indicates the reason for an update to a publication, as identified by the inclusion of a dcterms:date property (e.g. a change to the publication metadata)." .

:file-as					a rdf:Property ;
						rdfs:label "file-as";
						rdf:member :extended-props ;
						jena:prop-sort "50";
						rdfs:comment "A normalized form of the associated property for machine processing purposes." .

:role					a rdf:Property ;
						rdfs:label "role";
						rdf:member :extended-props ;
						jena:prop-sort "60";
						rdfs:comment "Indicates the role a contributor played in the creation of the content or production of the publication (values should be drawn from ONIX 3.0 Code List 17)." .

:scheme				a rdf:Property ;
						rdfs:label "scheme";
						rdf:member :extended-props ;
						jena:prop-sort "70";
						rdfs:comment "Indicates the scheme the associated property value conforms to (values should be drawn from ONIX 3.0 Code List 27)." .

:sort-order				a rdf:Property ;
						rdfs:label "sort-order";
						rdf:member :extended-props ;
						jena:prop-sort "80";
						rdfs:comment "Indicates the ordinal position in which to sort the current property relative to similarly-identified properties for the publication." .

:source-identifier			a rdf:Property ;
						rdfs:label "source-identifier";
						rdf:member :publication-props ;
						jena:prop-sort "90";
						rdfs:comment "Indicates which identifier property represents the source publication identifier." .



########## metadata authority properties #########

:meta-auth-props			a rdf:Bag ;
						jena:bag-sort "40" ;
						rdf:member :package-props ;
						dcterms:title "Metadata Authority properties" .

:authority				a rdf:Property ;
						rdfs:label "authority";
						rdf:member :meta-auth-props ;
						jena:prop-sort "10";
						rdfs:comment "The unique identifier for the metadata authority that issued an identifier for the publication." .

:meta-auth				a rdf:Property ;
						rdfs:label "meta-auth";
						rdf:member :meta-auth-props ;
						jena:prop-sort "30";
						rdfs:comment "The name of a metadata authority that issued an identifier for the publication" .

:signature				a rdf:Property ;
						rdfs:label "signature";
						rdf:member :meta-auth-props ;
						jena:prop-sort "40";
						rdfs:comment "Identifies an associated signature for the metadata authority available in the signatures.xml file." .


########## manifest properties #########

:manifest-props			a rdf:Bag ;
						jena:bag-sort "50" ;
						rdf:member :package-props ;
						dcterms:title "Manifest properties" .

:cover-image				a rdf:Property ;
						rdfs:label "cover-image";
						rdf:member :manifest-props ;
						jena:prop-sort "10";
						rdfs:comment "Indicates that the associated resource contains a representation of the cover art for the publication." .

:mathml				a rdf:Property ;
						rdfs:label "mathml";
						rdf:member :manifest-props ;
						jena:prop-sort "20";
						rdfs:comment "Indicates that the associated resource contains one or more instances of MathML markup." .

:online-resource			a rdf:Property ;
						rdfs:label "online-resource";
						rdf:member :manifest-props ;
						jena:prop-sort "30";
						rdfs:comment "Indicates that the associated resource contains references to additional resource that are not local to the publication." .

:page-progression-direction-ltr	a rdf:Property ;
						rdfs:label "page-progression-direction-ltr";
						rdf:member :manifest-props ;
						jena:prop-sort "40";
						rdfs:comment "Indicates that the page progression of the associated resource is from left to right." .

:page-progression-direction-rtl	a rdf:Property ;
						rdfs:label "page-progression-direction-rtl";
						rdf:member :manifest-props ;
						jena:prop-sort "50";
						rdfs:comment "Indicates that the page progression of the associated resource is from right to left." .

:scripted				a rdf:Property ;
						rdfs:label "scripted";
						rdf:member :manifest-props ;
						jena:prop-sort "60";
						rdfs:comment "Indicates that the associated resource contains scripted components." .

:svg					a rdf:Property ;
						rdfs:label "svg";
						rdf:member :manifest-props ;
						jena:prop-sort "70";
						rdfs:comment "Indicates that the associated resource contains one or more instances of SVG markup." .
