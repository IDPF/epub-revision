@base  <http://www.idpf.org/epub/30/vocab/package/> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix zt:  <http://www.daisy.org/ns/rdf/property#> .
@prefix htu: <http://www.daisy.org/ns/rdf/usage/html/#> .                
@prefix dcterms:  <http://purl.org/dc/terms/> .
@prefix jena: <http://www.daisy.org/ns/rdf/jena#> .
@prefix foaf: <http://xmlns.com/foaf/spec/#term_> .
@prefix aria: <http://www.w3.org/WAI/ARIA/schemata/aria-1.rdf#> .
@prefix xhv: <http://www.w3.org/1999/xhtml/vocab/#> .
@prefix role: <http://www.w3.org/1999/xhtml/vocab#> .
@prefix : <#> .

<>
  dcterms:title "EPUB3 Package Metadata Vocabulary" ;
  dcterms:description "This vocabulary, a part of EPUB 3.0, defines a set of metadata properties for use in [../../spec/epub30-publications.html#sec-package-documents Package Documents]." ;
  dcterms:publisher "International Digital Publishing Forum (IDPF)" ;
  dcterms:issued "2011-01-03" ;  
  dcterms:creator "mgylling" . 
    

########## package metadata #########

:package-props			a rdf:Bag ;
						jena:bag-sort "10" ;
						dcterms:title "Package-level Metadata" .


:content-version			a rdf:Property ;
						rdfs:label "content-version";
						rdf:member :package-props ;
						jena:prop-sort "10";
						zt:value "A string of the form #.#.# or similar" ;
						rdfs:comment "The version number of the package." .

:publication-type			a rdf:Property ;
						rdfs:label "publication-type";
						rdf:member :package-props ;
						jena:prop-sort "20";
						zt:value "dictionary | annotation" ;
						rdfs:comment "Describes the nature of a specialized type of EPUB Package." .

:source-identifier			a rdf:Property ;
						rdfs:label "source-identifier";
						rdf:member :package-props ;
						jena:prop-sort "30";
						zt:value "xsd:IDREF" ;
						rdfs:comment "References the identifier property that is the source of the NCX pageList." .

:page-progression-direction             a rdf:Property ;
						rdfs:label "page-progression-direction";
						rdf:member :package-props ;
						jena:prop-sort "40";
						zt:value "{rtl} | {ltr}" ;
						rdfs:comment "Indicates the direction in which content flows. !Are page-progression-direction-ltr and -rtl for manifest item properties duplication or a separate case?^" .



########## extended metadata properties #########

:extended-props			a rdf:Bag ;
						jena:bag-sort "20" ;
						dcterms:title "Extended metadata properties" .

:alternate-script			a rdf:Property ;
						rdfs:label "alternate-script";
						rdf:member :extended-props ;
						jena:prop-sort "10";
						zt:extends "dcterms:title" ;
						zt:extends "dcterms:creator" ;
						zt:extends "dcterms:contributor" ;
						rdfs:comment "Provides an alternate expression of the associated property in a language and script identified by the {xml:lang} attribute.!Only three properties?^" .

:date-ref				a rdf:Property ;
						rdfs:label "date-ref";
						rdf:member :extended-props ;
						jena:prop-sort "20";
						zt:extends "all" ;
						zt:value "xsd:IDREF" ;
						rdfs:comment "Relates a metadata property to a {dcterms:date} property. !Why was this under metadata authority props? Is it global?^" .

:display-seq				a rdf:Property ;
						rdfs:label "display-seq";
						rdf:member :extended-props ;
						jena:prop-sort "30";
						zt:extends "all" ;
						zt:value "xsd:unsignedInt" ;
						rdfs:comment "Indicates the order in which to display the current property relative to similarly-identified metadata properties." .

:event					a rdf:Property ;
						rdfs:label "event";
						rdf:member :extended-props ;
						jena:prop-sort "40";
						zt:extends "dcterms:date" ;
						zt:value "meta-update | !TODO: no other values or semi-controlled list?^" ;
						rdfs:comment "Describes a {dcterms:date} property." .

:file-as					a rdf:Property ;
						rdfs:label "file-as";
						rdf:member :extended-props ;
						jena:prop-sort "50";
						zt:extends "all" ;
						rdfs:comment "The normalized form of the associated property for sorting." .

:override				a rdf:Property ;
						rdfs:label "override";
						rdf:member :extended-props ;
						jena:prop-sort "55";
						zt:extends "DCMES:*" ;
						zt:value "xsd:IDREF" ;
						rdfs:comment "!TODO: Should this be a property and an attribute or just one of?^" .

:role					a rdf:Property ;
						rdfs:label "role";
						rdf:member :extended-props ;
						jena:prop-sort "60";
						zt:extends "dcterms:creator" ;
						zt:extends "dcterms:contributor" ;
						zt:value "It is recommended that values be drawn from MARC Code List for Relators and ONIX 3.0 Code List 17" ;
						rdfs:comment "Describes the nature of work performed by a creator or contributor." .

:scheme				a rdf:Property ;
						rdfs:label "scheme";
						rdf:member :extended-props ;
						jena:prop-sort "70";
						zt:extends "dcterms:identifier" ;
						zt:extends "dcterms:subject" ;
						zt:value "!TODO: note different schemes for different types or leave unspecified?^" ;
						rdfs:comment "Identifies the formal system used to express {dcterms:identifier} or {dcterms:subject} metadata." .

:sort-order				a rdf:Property ;
						rdfs:label "sort-order";
						rdf:member :extended-props ;
						jena:prop-sort "80";
						zt:extends "all" ;
						zt:value "xsd:unsignedInt" ;
						rdfs:comment "Indicates the position in which to sort the current property relative to similarly-identified properties for the package." .



########## metadata authority properties #########

:meta-auth-props			a rdf:Bag ;
						jena:bag-sort "30" ;
						dcterms:title "Metadata Authority properties" .

:authority				a rdf:Property ;
						rdfs:label "authority";
						rdf:member :meta-auth-props ;
						jena:prop-sort "10";
						zt:value "xsd:IDREF" ;
						rdfs:comment "References the metadata authority ({meta-auth}) responsible for an instance of package metadata." .

:meta-auth				a rdf:Property ;
						rdfs:label "meta-auth";
						rdf:member :meta-auth-props ;
						jena:prop-sort "20";
						rdfs:comment "The name of a party or authority responsible for the package metadata." .

:signature				a rdf:Property ;
						rdfs:label "signature";
						rdf:member :meta-auth-props ;
						jena:prop-sort "30";
						zt:extends "meta-auth" ;
						zt:value "xsd:IDREF" ;
						rdfs:comment "Relates a metadata authority ({meta-auth}) responsible for creating or updating package metadata to an XML Signature contained in the Signatures.xml file." .


########## manifest properties #########

:manifest-props			a rdf:Bag ;
						jena:bag-sort "40" ;
						dcterms:title "Manifest properties" .

:cover-image				a rdf:Property ;
						rdfs:label "cover-image";
						rdf:member :manifest-props ;
						jena:prop-sort "10";
						zt:for "item" ;
						zt:value "N/A" ;
						rdfs:comment "Identifies a resource as the cover image for the publication." .

:mathml				a rdf:Property ;
						rdfs:label "mathml";
						rdf:member :manifest-props ;
						jena:prop-sort "20";
						zt:for "item" ;
						zt:value "N/A" ;
						rdfs:comment "Indicates that the associated resource contains one or more instances of MathML markup." .

:online-resources			a rdf:Property ;
						rdfs:label "online-resources";
						rdf:member :manifest-props ;
						jena:prop-sort "30";
						zt:for "item" ;
						zt:value "N/A" ;
						rdfs:comment "Indicates that the associated resource itself references external resources." .

:scripted				a rdf:Property ;
						rdfs:label "scripted";
						rdf:member :manifest-props ;
						jena:prop-sort "40";
						zt:for "item" ;
						zt:value "N/A" ;
						rdfs:comment "Indicates that the associated resource contains scripted content." .

:svg					a rdf:Property ;
						rdfs:label "svg";
						rdf:member :manifest-props ;
						jena:prop-sort "50";
						zt:for "item" ;
						zt:value "N/A" ;
						rdfs:comment "Indicates that the associated resource contains one or more instances of SVG markup." .


########## metadata record properties #########

:metarec-props			a rdf:Bag ;
						jena:bag-sort "50" ;
						dcterms:title "Metadata record types" .

:onix-record				a rdf:Property ;
						rdfs:label "onix-record";
						rdf:member :metarec-props ;
						jena:prop-sort "10";
						zt:for "link" ;
						zt:value "N/A" ;
						rdfs:comment "Indicates the referenced resource is an ONIX record." .

:mods-record				a rdf:Property ;
						rdfs:label "mods-record";
						rdf:member :metarec-props ;
						jena:prop-sort "20";
						zt:for "link" ;
						zt:value "N/A" ;
						rdfs:comment "Indicates the referenced resource is a MODS record." .

:xmp-record				a rdf:Property ;
						rdfs:label "xmp-record";
						rdf:member :metarec-props ;
						jena:prop-sort "30";
						zt:for "link" ;
						zt:value "N/A" ;
						rdfs:comment "Indicates the referenced resource is an XMP record." .

:marc21xml-record			a rdf:Property ;
						rdfs:label "marc21xml-record";
						rdf:member :metarec-props ;
						jena:prop-sort "40";
						zt:for "link" ;
						zt:value "N/A" ;
						rdfs:comment "Indicates the referenced resource is a MARC21 record." .

