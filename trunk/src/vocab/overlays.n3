########## package metadata #########

:package-meta                           a rdf:Bag ;
					                       jena:bag-sort "10" ;
					                       dcterms:title "Package Metadata" ; 
					                       dcterms:description "Properties for inclusion in EPUB 3 Package Documents".
					
:duration                               a rdf:Property ;
					                       rdfs:label "duration";
					                       rdf:member :package-meta ;
					                       jena:prop-sort "10" ;
					                       rdfs:comment "Clock value expressed in hh:mm:ss.s syntax or as a single unit of h (hours), min (minutes), s (seconds), or ms (milliseconds).  A subset of [[http://www.w3.org/TR/SMIL/smil-timing.html#q31 SMIL Wallclock-sync values]]" .							

:narrator                                a rdf:Property ;
					                       rdfs:label "narrator";
					                       rdf:member :package-meta ;
					                       jena:prop-sort "20" ;
					                       rdfs:comment "String containing the name of the narrator." .
